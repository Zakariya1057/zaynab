{
  "expo": {
    "name": "Zaynab",
    "slug": "zaynab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "icon": "./assets/logo.png",
      "splash": {
        "image": "./assets/android-splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#9039cf"
      },
      "supportsTablet": true,
      "bundleIdentifier": "zakariyamohummed.zaynab.com",
      "infoPlist": {
        "UIBackgroundModes": ["audio", "fetch"],
        "NSUserNotificationUsageDescription": "This app uses notifications to remind you to finish your episode."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#9039cf"
      },
      "package": "zakariyamohummed.zaynab.com",
      "permissions": [
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "splash": {
        "image": "./assets/android-splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#9039cf"
      }
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      ["@morrowdigital/watermelondb-expo-plugin"],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.6.10",
            "packagingOptions": {
              "pickFirst": ["**/libc++_shared.so"]
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "950794c4-275a-4f87-a0a3-c5df8c91c6a1"
      }
    }
  }
}
